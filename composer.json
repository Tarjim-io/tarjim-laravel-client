{
    "name": "joylab/tarjim-laravel-client",
    "version": "0.1.0",
    "type": "library",
    "description": "Laravel SDK for Tarjim (https://tarjim.io/)",
    "keywords": [
        "laravel",
        "tarjim",
        "translations"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tarjim\\Laravel\\": "src/"
        }
    },
    "require": {
        "joylab/tarjim-php-client": "^1.1",
        "illuminate/support": "^6.0 | ^7.0 | ^8.0 | ^9.0 | ^10.0 | ^11.0",
        "php": "^7.2 | ^8.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarjim\\Laravel\\ServiceProvider"
            ]
        }
    }
}