{
  "name": "joylab/tarjim-laravel-client",
  "type": "library",
  "description": "Laravel SDK for Tarjim (https://tarjim.io/)",
  "keywords": [
    "laravel",
    "tarjim",
    "translations"
  ],
  "license": "MIT",
  "require": {
    "joylab/tarjim-php-client": "^1.1",
    "illuminate/support": "^6.0 | ^7.0 | ^8.0 | ^9.0 | ^10.0",
    "php": "^7.2 | ^8.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "autoload": {
    "psr-0": {
      "Tarjim\\Laravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tarjim\\Laravel\\ServiceProvider"
      ]
    }
  }
}
